#!/usr/bin/env bash
# shellcheck disable=SC1091

set -euo pipefail
cd "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )/.."
. 'script/utils.sh'

run "${PWD}/script/setup"

. 'script/import.sh'

h1 'Generate requirements.txt'
run 'pipenv run pip freeze > requirements.txt'

h1 'Install clowder as user'
run pip install --user -e .
